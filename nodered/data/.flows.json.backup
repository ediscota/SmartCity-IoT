    {
        "id": "4b32dc2d835769b2",
        "type": "function",
        "z": "f6f2187d.f17ca8",
        "name": "function: prepare influx",
        "func": "let district, street, sensor, value, timestamp;\n\n// Caso 1: sensori grezzi (dopo parse topic)\nif (msg.sensor && msg.district && msg.street) {\n    district = msg.district;\n    street   = msg.street;\n    sensor   = msg.sensor;\n    value    = parseFloat(msg.payload.value);\n\n    if (msg.payload.timestamp) {\n        // sensori Python: timestamp in secondi\n        timestamp = new Date(msg.payload.timestamp * 1000);\n    } else {\n        timestamp = new Date();\n    }\n}\n// Caso 2: AQI (topic: smartcity/processed/<district>/<street>/aqi)\nelse if (msg.topic && msg.topic.endsWith(\"/aqi\")) {\n    let parts = msg.topic.split(\"/\");\n    district = parts[2];\n    street   = parts[3];\n    sensor   = \"aqi\";\n\n    value = parseFloat(msg.payload.value);\n\n    if (msg.payload.timestamp) {\n        // AQI usa giÃ  millisecondi\n        timestamp = new Date(msg.payload.timestamp);\n    } else {\n        timestamp = new Date();\n    }\n} else {\n    // messaggio non gestibile\n    return null;\n}\n\n// ---- VALIDAZIONE ----\nif (isNaN(value)) {\n    node.warn(\"Scarto punto: value non numerico\");\n    return null;\n}\n\nif (!(timestamp instanceof Date)) {\n    node.warn(\"Scarto punto: timestamp non valido\");\n    return null;\n}\n\n// FORMAT per InfluxDB: [fields, tags]\nlet fields = {\n    value: value\n};\n\nlet tags = {\n    district: district,\n    street: street,\n    sensor: sensor\n};\n\nmsg.measurement = \"smartcity_sensors\";\nmsg.payload = [fields, tags];\nmsg.timestamp = timestamp;\n\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 1260,
        "y": 300,
